package evacuation_simulation;

import jade.core.AID;
import repast.simphony.random.RandomHelper;

public class IndependentAltruistic extends Person{

	public IndependentAltruistic(AID resultsCollector){
		super(resultsCollector);
				
		independent = RandomHelper.nextIntFromTo(MIN_SCALE, MAX_SCALE/2);
		areaKnowledgeable = RandomHelper.nextIntFromTo(MAX_SCALE / 2, MAX_SCALE);
		altruistic = RandomHelper.nextIntFromTo(MAX_SCALE / 2, MAX_SCALE);
	}
	
	
}
