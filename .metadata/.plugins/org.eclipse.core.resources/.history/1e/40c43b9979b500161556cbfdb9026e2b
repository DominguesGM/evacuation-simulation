package serviceConsumerProviderVis.onto;

import jade.content.AgentAction;

public class DirectionsRequest implements AgentAction {
	
	private static final long serialVersionUID = 1L;
	
	private int x;
	private int y;

	public DirectionsRequest() {
	}
	
	public DirectionsRequest(int myX, int myY) {
		this.x = myX;
		this.y = myY;
	}

	/**
	 * @return the x
	 */
	public int getX() {
		return x;
	}

	/**
	 * @param x the x to set
	 */
	public void setX(int x) {
		this.x = x;
	}

	/**
	 * @return the y
	 */
	public int getY() {
		return y;
	}

	/**
	 * @param y the y to set
	 */
	public void setY(int y) {
		this.y = y;
	}
}
